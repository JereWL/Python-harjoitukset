sukupuoli = input("Enter biological gender (male/female): ")
hemoglobin = input("Enter hemoglobin value (g/l): ")

if sukupuoli.lower() == ("male") :
    hemoglobin = float(hemoglobin)
    if hemoglobin < 134:
        print("Your hemoglobin is low.")
    elif 134 <= hemoglobin <= 167:
        print("Your hemoglobin is normal.")
    else:
        print("Your hemoglobin is high.")
elif sukupuoli.lower() == ("female") :
    hemoglobin = float(hemoglobin)
    if hemoglobin < 117:
        print("Your hemoglobin is low.")
    elif 117 <= hemoglobin <= 155:
        print("Your hemoglobin is normal.")
    else:
        print("Your hemoglobin is high.")
else:
    print("Invalid gender.")